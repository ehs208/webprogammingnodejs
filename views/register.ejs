<div class="container">	
    <h2>회원가입</h2>
    <form action="/auth/register_process" method="post">
        <div class="mb-3">
            <label class="form-label" for="id">아이디</label> 
            <input class="form-control" type="text" name="id" id="id" style="width:300px;" />
        </div>
        <div class="mb-3">
            <label class="form-label" for="pwd">비밀번호</label> 
            <input class="form-control" type="password" name="pwd" id="pwd" style="width:300px;"/>
        </div>
        <div class="mb-3">
            <label class="form-label" for="name">이름</label> 
            <input class="form-control" type="text" name="name" id="name" style="width:300px;"/>
        </div>
        <div class="mb-3">
            <label class="form-label" for="address">주소</label> 
            <input class="form-control" type="text" name="address" id="address" style="width:300px;"/>
        </div>
        <div class="mb-3">
            <label class="form-label" for="phone">번호</label> 
            <input class="form-control" type="text" name="phone" id="phone" style="width:300px;"/>
        </div>
        <div class="mb-3">
            <label class="form-label" for="birth">생일</label> 
            <input class="form-control" type="text" name="birth" id="birth" style="width:300px;"/>
        </div>
        <div class="mb-3">
            <label class="form-label" for="class">Class</label>
            <select class="form-select" name="class" id="class" style="width:300px;">
                <option value="00">00</option>
                <option value="01">01</option>
                <option value="02">02</option>
            </select>
        </div>
        <button class="btn btn-outline-primary btn-sm" type="submit">회원가입</button>
    </form>
</div>
